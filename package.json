{
  "name": "covid-cli-19",
  "version": "1.0.4",
  "description": "simple cli app that show covid stats",
  "main": "index.js",
  "author": "Isa Toltar",
  "license": "MIT",
  "keywords": ["covid-cli","covid-19","inquirer","cli-tool"],
  "scripts": {
